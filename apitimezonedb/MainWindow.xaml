<Window x:Class="apitimezonedb.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:apitimezonedb"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="500">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBox x:Name="MiastoTextBox" Width="200" Height="25"/>
            <Button x:Name="PobierzCzasButton" Content="Pobierz Czas" Width="120" Height="25" Click="PobierzCzasButton_Click"/>
        </StackPanel>

        <ScrollViewer>
            <ListBox x:Name="CzasListBox" Width="480" MinHeight="200"/>
        </ScrollViewer>
    </StackPanel>

</Window>
